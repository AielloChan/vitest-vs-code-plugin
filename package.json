{
  "name": "vscode-vitest",
  "version": "1.0.0",
  "main": "dist/index.js",
  "engines": {
    "vscode": "^1.65.0"
  },
  "repository": "git@github.com:kwai-explore/vscode-vitest.git",
  "author": "wangwenlu <wangwenlu@kuaishou.com>",
  "license": "MIT",
  "activationEvents": [
    "onLanguage:typescript"
  ],
  "devDependencies": {
    "esbuild": "^0.14.27",
    "prettier": "^2.6.0",
    "typescript": "^4.6.2",
    "vsce": "^2.7.0",
    "vscode": "^1.1.37"
  },
  "scripts": {
    "build:dts": "tsc",
    "build:bundle": "node ./scripts/build.js",
    "build": "yarn build:bundle && yarn build:dts",
    "format:check": "prettier --config ./prettier.config.json --c ./src/**/*.ts",
    "format:write": "yarn format:check --write"
  }
}
